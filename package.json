    "tailwindcss": "^3.4.1",
    "typescript": "~5.4.3",
    "vite": "^5.2.6",
    "vite-plugin-vue-devtools": "^7.0.21",
    "vitest": "^1.3.1",
    "vue-tsc": "^2.0.7"
